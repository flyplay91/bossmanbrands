      }

      .ambassador-hero-video {
        height: 350px;
        width: 480px;
        background-size: cover;
        position: absolute;
        border: 10px solid #C29555;
        top: 50%;
        transform: translateY(-50%);
        left: 0;
        z-index: 1;
        background-repeat:no-repeat;
        background-position:center;
        @media(max-width: 991px) {
          width: 380px;
        }
        @media(max-width: 767px) {
          position: unset;
          transform: unset;
        }

        button {
          border: none;
          position: relative;
          top: 50%;
          left: 40%;
          cursor: pointer;
          width: 90px;
          height: 90px;
          background-size: cover;
          background-repeat: no-repeat;
          background-color: transparent;
          transform: translateY(-50%);

        }
      }

      .ambassador-hero-content {
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        right: 0;
        border: 10px solid #C29555;
        width: 89%;
        height: 450px;
        margin-left: auto;
        @media(max-width: 767px) {
          width: 100%;
          border: none;
          position: unset;
          transform: unset;
          height: auto;
          margin-top: 50px;
          text-align: center;
        }
        .ambassador-video--content {
          position: absolute;
          top: 50%;
          transform: translateY(-50%);
          left: 52%;
          @media(max-width: 991px) {
            left: 47%;
          }
          @media(max-width: 767px) {
            position: unset;
            transform: unset;
          }
          h5 {
            color: #C29555;
            font-size: 45px;
            margin: 0;
            font-family: bely, sans-serif;
            line-height: 0.8;
            font-weight: 400;
          }

          h1 {
