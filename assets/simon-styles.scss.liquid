            font-family: bely, sans-serif;
            line-height: 1;
            @media(max-width: 991px) {
              font-size: 120px;
            }
          }

          a {
            background-color: #C29555;
            color: white;
            border: 0px solid white;
            padding: 20px 45px;
            display: inline-block;
            text-decoration: none;
            font-weight: bold;
            font-size: 21px;
