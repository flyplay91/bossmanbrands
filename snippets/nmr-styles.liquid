    }
    /* end this needs to be here */


    /* nmr product card */
    .nmr-butt,
    button.nmr-butt {
        transition-duration: 200ms;
        border-radius: 3px;
        line-height: 1;
        padding: 0.5em 1em;
        border: solid 1px #C0964A !important;
        background-color: #ffffff;
        color: #C0964A;
        text-transform: uppercase;
        text-align: center;
        font-weight: bold;

        position: relative;
    }

    .nmr-butt.nmr-butt-white,
    button.nmr-butt.nmr-butt-white {
        background-color: #C0964A;
        border: solid 1px #FFFFFF !important;
        color: #FFFFFF;
    }

    .nmr-butt.is-adding,
    button.nmr-butt.is-adding {
        border: solid 1px #C0964A !important;
        background-color: #C0964A;
        color: #FFFFFF;
    }

    .nmr-butt.is-added,
    button.nmr-butt.is-added {
        border: solid 1px #05901f !important;
        background-color: #05901f;
        color: #FFFFFF;
    }

    /* nmr button icon */
    .nmr-butt .icon-added,
    button.nmr-butt .icon-added,
    .nmr-butt .icon-adding,
    button.nmr-butt .icon-adding {
        transition-duration: 200ms;
        right: 0;
        top: 50%;
        transform: translate(175%, -50%);
        color: #C0964A !important;

        position: absolute;
        display: none;
    }

    .nmr-butt.is-adding .icon-adding,
    button.nmr-butt.is-adding .icon-adding,
    .nmr-butt.is-added .icon-added,
    button.nmr-butt.is-added .icon-added {
        display: block;
    }

    .nmr-butt.nmr-butt-white.is-adding,
    button.nmr-butt.nmr-butt-white.is-adding {
        color: #C0964A !important;
        background-color: #FFFFFF !important;
        border: solid 1px #FFFFFF !important;

    }

    .nmr-butt.nmr-butt-white.is-adding .icon-adding,
    button.nmr-butt.nmr-butt-white.is-adding .icon-adding,
    .nmr-butt.nmr-butt-white.is-added .icon-added,
    button.nmr-butt.nmr-butt-white.is-added .icon-added {
        color: #FFFFFF !important;
    }

    .nmr-butt.nmr-butt-white.is-added,
    button.nmr-butt.nmr-butt-white.is-added {
        border: solid 1px #05901f !important;
    }

    .nmr-butt.nmr-butt-white.is-adding .icon-adding,
    button.nmr-butt.nmr-butt-white.is-adding .icon-adding,
    .nmr-butt.nmr-butt-white.is-added .icon-added,
    button.nmr-butt.nmr-butt-white.is-added .icon-added {
        color: #FFFFFF !important;
    }

