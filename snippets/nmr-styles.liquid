    /* Tiny Slider Overrides */

    .nmr-image-landscape {
        background-color: #efefef;
        display: block;
        content: "";
        padding-top: 60%;
        position: relative;
    }

    .tns-outer {
        position: relative;
    }

    .tns-nav {
        display: none;
    }

    .tns-controls {
        display: flex;
        justify-content: center;
        align-items: center;
        z-index: 99;

        outline: 0;

        position: absolute;
        bottom: 0;
        width: 100%;
        transform: translateY(50%);
    }

    .nmr-olive-left,
    .nmr-olive-right {
        position: absolute;
        bottom: 0;
        height: 4em;
        width: 4em;
        background-size: contain;
        background-position: center;
        background-repeat: no-repeat;
    }

    .nmr-olive-left {
        left: 0;
        transform: translate(18%, -20%);
    }

    .nmr-olive-right {
        right: 0;
        transform: translate(-18%, -20%);
    }

    button[data-controls=next],
    button[data-controls=prev] {
        cursor: pointer;
        border-radius: 999999px;
        width: 2rem;
        height: 2rem;
        background-color: #C0964A;
        color: white;
        margin: 0 0.25rem;

        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;

        outline: 0;
        box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06);
        transform: translateY(0);
        transition-duration: 200ms;
    }

    button[data-action=start],
    button[data-action=stop] {
        display: none !important;
    }

    button[data-controls=next]:hover,
    button[data-controls=prev]:hover {
        box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05);
        transform: translateY(-10%);
    }

    button,
    button:hover,
    button:focus,
    button:active,
    button .fas,
    button:hover .fas,
    button:focus .fas,
    button:active .fas {
        outline: 0 !important;
    }


    /* nmr product-type tabs css */
    #nmr-tabs-2 {
        position: relative;
    }

