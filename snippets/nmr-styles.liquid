    .nmr-image:after {
        padding-top: 150%;
    }

    .nmr-image-square:after {
        padding-top: 100%
    }

    .nmr-cta {}

    main .fas,
    .far {
        font-size: inherit !important;
        color: inherit !important;
    }

    main button .fas {
        font-size: 0.8rem;
    }

    #shopify-section-nmr-module-product-info {
        margin: 5em 0;
    }

    .nmr-para-sm,
    .nmr-para,
    .nmr-para-lg {
        display: block;
    }

    .nmr-para-sm {
